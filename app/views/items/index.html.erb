<h1>Bienvenue dans Catto</h1>

     <% @items.each do |item| %>
	     <p>Titre: "<%= item.title %>"</p> 
	     <p>Description: "<%= item.description %>"</p> 
	     <p>Prix: "<%= item.price %> $"</p> 
	     <p>Image_url :  "<%= item.image_url %>"</p> 
	     <button> <%= link_to "afficher", item_path(item.id)%> </button>
	     <h1>nextItem</h1>
     <%end%>